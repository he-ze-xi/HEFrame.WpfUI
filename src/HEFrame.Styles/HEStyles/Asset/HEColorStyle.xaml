<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--  Background  -->
    <!--  Dark  -->
    <Color x:Key="DarkBackgroundColor">#393939</Color>
    <SolidColorBrush x:Key="DarkBackground" Color="#393939" />
    <SolidColorBrush x:Key="DarkMouseHover" Opacity="0.9" Color="#393939" />
    <SolidColorBrush x:Key="DarkPressed" Opacity="0.7" Color="#393939" />
    <!--  Light  -->
    <Color x:Key="LightColor">White</Color>
    <SolidColorBrush x:Key="LightBackground" Color="#C0C0C0" />
    <SolidColorBrush x:Key="LightMouseHover" Opacity="0.9" Color="#C0C0C0" />
    <SolidColorBrush x:Key="LightPressed" Opacity="0.7" Color="#C0C0C0" />
    <!--  Primary  -->
    <SolidColorBrush x:Key="PrimaryBackground" Color="#80B9EE" />
    <SolidColorBrush x:Key="PrimaryMouseHover" Opacity="0.9" Color="#80B9EE" />
    <SolidColorBrush x:Key="PrimaryPressed" Opacity="0.7" Color="#80B9EE" />
    <!--  Success  -->
    <SolidColorBrush x:Key="SuccessBackground" Color="#4CAF50" />
    <SolidColorBrush x:Key="SuccessMouseHover" Opacity="0.9" Color="#4CAF50" />
    <SolidColorBrush x:Key="SuccessPressed" Opacity="0.7" Color="#4CAF50" />
    <!--  Caution  -->
    <SolidColorBrush x:Key="CautionBackground" Color="#FF9800" />
    <SolidColorBrush x:Key="CautionMouseHover" Opacity="0.9" Color="#FF9800" />
    <SolidColorBrush x:Key="CautionPressed" Opacity="0.7" Color="#FF9800" />
    <!--  Dranger  -->
    <SolidColorBrush x:Key="DrangerBackground" Color="#F44336" />
    <SolidColorBrush x:Key="DrangerMouseHover" Opacity="0.9" Color="#F44336" />
    <SolidColorBrush x:Key="DrangerPressed" Opacity="0.7" Color="#F44336" />
    <!--  Transparent  -->
    <SolidColorBrush x:Key="TransparentBackground" Color="Transparent" />


    <!--  Dark  -->

    <Color x:Key="DarkForegroundColor">Black</Color>
    <SolidColorBrush x:Key="DarkForeground" Color="{StaticResource DarkForegroundColor}" />
    <!--  Light  -->

    <SolidColorBrush x:Key="LightColorBrush" Color="{StaticResource LightColor}" />
    <Color x:Key="LightForegroundColor">White</Color>
    <SolidColorBrush x:Key="LightForeground" Color="White" />
    <!--  Primary  -->
    <Color x:Key="PrimaryColor">#80B9EE</Color>
    <SolidColorBrush x:Key="PrimaryForeground" Color="#80B9EE" />
    <!--  Warm  -->
    <Color x:Key="WarmColor">#FF9800</Color>
    <SolidColorBrush x:Key="WarmBrush" Color="{StaticResource WarmColor}" />
    <!--  Dranger  -->
    <Color x:Key="DrangerColor">#F44336</Color>
    <SolidColorBrush x:Key="DrangerBrush" Color="{StaticResource DrangerColor}" />

    <Color x:Key="FillColor">#CCCCCC</Color>
    <SolidColorBrush x:Key="DefaultFillColor" Color="#CCCCCC" />

    <Color x:Key="DarkMouseOverColor">#505050</Color>
    <SolidColorBrush x:Key="DarkMouseOver" Color="{StaticResource DarkMouseOverColor}" />
    <Color x:Key="LightMouseOverColor">#D0D0D0</Color>
    <SolidColorBrush x:Key="LightMouseOver" Color="{StaticResource LightMouseOverColor}" />

    <Color x:Key="DarkContrastColor">#D0D0D0</Color>
    <SolidColorBrush x:Key="DarkContrast" Color="{StaticResource DarkContrastColor}" />
    <Color x:Key="LightContrastColor">#505050</Color>
    <SolidColorBrush x:Key="LightContrast" Color="{StaticResource LightContrastColor}" />

    <Color x:Key="MenuDefaultColor">#393D49</Color>
    <SolidColorBrush x:Key="MenuDefault" Color="{StaticResource MenuDefaultColor}" />
</ResourceDictionary>